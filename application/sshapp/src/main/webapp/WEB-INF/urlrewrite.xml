<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
"urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
  <rule>
    <from>/img/**</from>
    <to>/img/$1</to>
  </rule>
  <rule>
    <from>/js/**</from>
    <to>/js/$1</to>
  </rule>
  <rule>
    <from>/ext/**</from>
    <to>/ext/$1</to>
  </rule>
  <rule>
    <from>/css/**</from>
    <to>/css/$1</to>
  </rule>
  <rule>
    <from>/lbClassicStatus/**</from>
    <to>/lbClassicStatus/$1</to>
  </rule>
  <!--example book-->
  <rule>
    <from>/book/**</from>
    <to>/book/$1</to>
  </rule>

  <!--remote-->
  <rule>
    <from>/remote/**</from>
    <to>/remote/$1</to>
  </rule>

  <rule>
    <from>/index2.jsp</from>
    <to>/index2.jsp</to>
  </rule>
  <rule>
    <from>/index.jsp</from>
    <to>/index.jsp</to>
  </rule>
  <rule>
    <from>/common/**</from>
    <to>/common/$1</to>
  </rule>

  <rule>
    <from>/**</from>
    <to>/rest/$1</to>
  </rule>
  <outbound-rule>
    <from>/rest/**</from>
    <to>/$1</to>
  </outbound-rule>
</urlrewrite>
